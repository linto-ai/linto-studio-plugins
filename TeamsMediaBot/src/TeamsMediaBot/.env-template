# Rename this to .env and add the values
# This is only used for local development
# When deployed to Azure or running as a service locally, 
# the deployment stores these settings in Key Vault
# and DSC will add them to the VM Environment Variables
# Remove this comment
AppSettings__AadAppId=
AppSettings__AadAppSecret=
AppSettings__ServiceDnsName=bot.example.com
AppSettings__CertificateThumbprint=
AppSettings__BotCallingInternalPort=9442
AppSettings__BotInstanceExternalPort=443
AppSettings__BotInternalPort=9441
AppSettings__MediaInstanceExternalPort=22210
AppSettings__MediaInternalPort=8445
AppSettings__ApplicationInsightsInstrumentationKey=
AppSettings__UseLocalDevSettings=true
AppSettings__MediaDnsName=tcp.example.com

# MQTT Configuration
AppSettings__BrokerHost=localhost
AppSettings__BrokerPort=1883
AppSettings__BrokerUsername=
AppSettings__BrokerPassword=
AppSettings__BrokerKeepAlive=60
AppSettings__BrokerUseTls=false
AppSettings__BrokerAllowUntrustedCertificates=false
# Transport protocol: Tcp (default), WebSocket (ws://), or SecureWebSocket (wss://)
AppSettings__BrokerProtocol=Tcp
# WebSocket path (only used with WebSocket or SecureWebSocket protocol)
AppSettings__BrokerWebSocketPath=/mqtt
AppSettings__BotDisplayName=Transcription Bot