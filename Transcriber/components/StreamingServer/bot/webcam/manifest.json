{
    "name": "Virtual Webcam",
    "version": "1.0.0",
    "minimum_chrome_version": "10.0",
    "description": "Virtual webcam.",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "js/inject.js"
        ],
        "run_at": "document_start",
        "all_frames": true
      }
    ],
    "permissions": [
      "tabs",
      "<all_urls>"
    ],
    "manifest_version": 2,
    "web_accessible_resources": [
      "js/*",
      "js/linto.svg"
    ]
  }